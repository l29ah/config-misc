Tkabber*font: Terminus 8

Sxiv.background: #777777

Rxvt*background: black
Rxvt*foreground: white
Rxvt*font: xft:Terminus:pixelsize=14, xft:Arial Unicode MS:pixelsize=14
Rxvt*scrollWithBuffer: True
Rxvt*scrollTtyOutput: False
Rxvt*scrollTtyKeypress: True
Rxvt*scrollBar: false
Rxvt*preeditType: OnTheSpot
Rxvt*inputMethod: uim
Rxvt*perl-ext: default,selection
Rxvt*saveLines: 10000
Rxvt*urgentOnBell: True
!Rxvt*color0:			black
!Rxvt*color1:			#B00000
!Rxvt*color2:			green4
!Rxvt*color3:			yellow3
!Rxvt*color4:			blue3
urxvt*color4: #0000FF
!Rxvt*color5:			#A000A0
!Rxvt*color6:			#009999
!Rxvt*color7:			#D0B099
!Rxvt*color8:			grey90
!Rxvt*color9:			#FF3030
!Rxvt*color10:			#70FF70
!Rxvt*color11:			#F0F080
!Rxvt*color12:			#B0B0FF
!Rxvt*color13:			#EFA0BF
!Rxvt*color14:			#A0FFFF
!Rxvt*color15:			white

! Disable printing the terminal contents when pressing PrintScreen.
URxvt.print-pipe: "cat > /dev/null"

xterm*utf8Title: true
xterm*faceSize: 6
xterm*faceName: Terminus
xterm*colorBD:     #e6d51d
xterm*background:  #000000
xterm*foreground:  #ffffff
xterm*cursorColor: grey
xterm*scrollBar:        false
xterm*eightBitInput:    false
xterm*metaSendsEscape:  true
xterm*boldMode:         true
xterm*alwaysBoldMode:   false
xterm*dynamicColors:    on
xterm*colorMode:        on
xterm*saveLines:        20000
xterm*visualBell:       true
xterm*color12: #0000ff
xterm*scrollttyoutput: false
! makes doubleclick word selection work nicely
xterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48

Xft.dpi: 123
